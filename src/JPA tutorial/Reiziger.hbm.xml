<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="hu.nl.hibernate.Reiziger" table="Reiziger">
		<id name="ReizigerID" type="int" access="field">
			<column name="ReizigerID"/>
			<generator class="assigned"/>
		</id>
		<property name="naam" type="java.lang.String" access="field">
			<column name="Naam"/>
		</property>
		<property name="gbdatum" type="java.util.Date" access="field">
			<column name="Geboortedatum"/>
		</property>
	</class>
</hibernate-mapping>